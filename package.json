{
  "name": "deepcode",
  "//": "Changing display name requires change in general.ts",
  "displayName": "DeepCode (Beta)",
  "description": "Detect bugs and quality issues in JavaScript, TypeScript, React, Vue and more...",
  "version": "1.0.2",
  "icon": "images/deepCodeLogo.jpg",
  "publisher": "deepcode",
  "homepage": "https://www.deepcode.ai/",
  "repository": {
    "type": "git",
    "url": "https://github.com/DeepCodeAI/vscode-extension.git"
  },
  "engines": {
    "vscode": "^1.36.0"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "keywords": [
    "code analysis",
    "java",
    "javascript",
    "typescript",
    "python"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": [
      {
        "title": "DeepCode",
        "properties": {
          "deepcode.api.cloudBackend": {
            "type": "string",
            "default": "",
            "description": "In order to use a self-managed DeepCode server with Bitbucket or Gitlab instance, update the instance url below:",
            "scope": "application"
          }
        }
      }
    ],
    "commands": [
      {
        "command": "deepcode.start",
        "title": "DeepCode"
      },
      {
        "command": "deepcode.settings",
        "title": "DeepCode Settings"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "watch": "tsc -watch -p ./",
    "test-compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/nock": "^11.1.0",
    "@types/node": "^10.12.21",
    "nock": "^11.7.2",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.1",
    "typescript": "^3.7.5",
    "@types/vscode": "^1.41.0",
    "vscode-test": "^1.3.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "serialize-javascript": "^2.1.2",
    "mocha": "^7.0.0",
    "glob": "^7.1.6"
  },
  "dependencies": {
    "@types/git-repo-name": "^1.0.0",
    "@types/git-username": "^1.0.0",
    "@types/mz": "^0.0.32",
    "@types/request-promise": "^4.1.45",
    "git-repo-name": "^1.0.1",
    "git-username": "^1.0.0",
    "ignore": "^5.1.4",
    "mz": "^2.7.0",
    "open": "^7.0.0",
    "request-promise": "^4.2.5",
    "supports-color": "^7.1.0"
  }
}
